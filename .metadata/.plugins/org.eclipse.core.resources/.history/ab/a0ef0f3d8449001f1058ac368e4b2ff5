package StarPattern;

public class pattern10 
{
	public static void main(String[] args) {
		
	
//*****
//****
//***
//**
//*
//**
//***
//****
//*****
	int star=5;
	for(int i=1;i<=9;i++)
	{
	System.out.print("*");	
	}
	System.out.println();
	int i = 0;
	if(i<5)
	{
		star--;
	}
	else
	{
		star++;
	}
	}
	}
	

